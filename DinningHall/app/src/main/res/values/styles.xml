<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- 复选框样式 -->
    <style name="CheckboxTheme">
        <item name="android:button">@drawable/btn_check</item>
        <item name="android:paddingLeft">15.0dip</item>
    </style>
    <!-- checkbox样式-->
    <style name="CheckboxThemezhidao">
        <item name="android:button">@drawable/btn_check_zhidao</item>
        <item name="android:paddingLeft">5.0dip</item>
    </style>
    <style name="MIS_NO_ACTIONBAR" parent="Theme.AppCompat.Light.NoActionBar"></style>
</resources>
